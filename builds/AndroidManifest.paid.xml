<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jhavatar.softwareinterviewnotespaid"
    android:versionCode="5"
    android:versionName="1.0.4"
    >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:name="com.jhavatar.softwareinterviewnotes.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon_launch_pro"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:launchMode="singleInstance">
        <activity
            android:name="com.jhavatar.softwareinterviewnotes.MainMenu"
            android:label="@string/app_name"
        	android:configChanges="keyboardHidden|orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />  
            </intent-filter>       
        </activity>
        
    </application>

</manifest>
